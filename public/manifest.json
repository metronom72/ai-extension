{
  "manifest_version": 3,
  "name": "BriefCat Extension",
  "version": "1.0",
  "description": "BriefCat extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
